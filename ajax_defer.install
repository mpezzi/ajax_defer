/**
 * @file
 * Provides install / uninstall functionality for ajax_defer.module
 */

/**
 * Implements hook_uninstall().
 */
function ajax_defer_uninstall() {

  variable_del('ajax_defer_default_delay');
  variable_del('ajax_defer_storage_class');
  variable_del('ajax_defer_storage_expire');

}
